{
  "name": "oro/commerce-authorizenet",
  "description": "OroCommerce Authorize.Net integration",
  "homepage": "https://github.com/orocommerce/OroAuthorizeNetBundle.git",
  "license": "OSL-3.0",
  "authors": [
    {
      "name": "Oro, Inc",
      "homepage": "https://www.orocommerce.com"
    }
  ],
  "autoload": {
    "psr-4": {"Oro\\Bundle\\AuthorizeNetBundle\\": ""},
    "exclude-from-classmap": ["/Tests/"]
  },
  "require": {
    "oro/commerce": "6.1.*",
    "jms/serializer": "^1.2|^2.0|^3.0",
    "authorizenet/authorizenet": "~2.0.2"
  },
  "repositories": {
    "oro": {
      "type": "composer",
      "url": "https://packagist.orocrm.com"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "6.1-dev"
    }
  }
}
